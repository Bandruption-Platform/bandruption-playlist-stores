{
  "name": "bandruption-playlist-stores",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "test:ui": "vitest --ui",
    "test:mobile": "turbo run test --filter=mobile",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean",
    "db:generate": "turbo run db:generate",
    "db:push": "turbo run db:push",
    "web": "turbo run dev --filter=web",
    "mobile": "turbo run dev --filter=mobile",
    "mobile:ios": "cd apps/mobile && npm run dev -- --ios",
    "mobile:android": "cd apps/mobile && npm run dev -- --android",
    "mobile:web": "cd apps/mobile && npm run build:web",
    "server": "turbo run dev --filter=server",
    "setup": "npm install && npm run db:generate",
    "watchman-clear": "watchman watch-del-all",
    "dev:clean": "npm run watchman-clear && npm run dev",
    "mobile:clean": "cd apps/mobile && npm run dev:clean"
  },
  "dependencies": {
    "react": "19.0.0",
    "react-dom": "19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.9.1",
    "globals": "^15.9.0",
    "prettier": "^3.0.0",
    "turbo": "^1.13.3",
    "typescript": "^5.1.6",
    "typescript-eslint": "^8.3.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  }
}
